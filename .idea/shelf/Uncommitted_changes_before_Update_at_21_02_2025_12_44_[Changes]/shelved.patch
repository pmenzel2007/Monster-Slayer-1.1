<+>class GameObject {\r\n    constructor(x, y, width, height) {\r\n        this.x = x;\r\n        this.y = y;\r\n        this.width = width;\r\n        this.height = height;\r\n    }\r\n\r\n    collidesWith(other) {\r\n        return (\r\n            this.x < other.x + other.width &&\r\n            this.x + this.width > other.x &&\r\n            this.y < other.y + other.height &&\r\n            this.y + this.height > other.y\r\n        );\r\n    }\r\n\r\n    draw(ctx) {\r\n        ctx.fillStyle = \"blue\";\r\n        ctx.fillRect(this.x, this.y, this.width, this.height);\r\n    }\r\n}
