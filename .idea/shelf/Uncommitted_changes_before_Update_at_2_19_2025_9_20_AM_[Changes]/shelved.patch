+        let oldX = this.x;
+        let oldY = this.y;
+
+        let distanceX = Math.abs(playerX - this.x);
+        let distanceY = Math.abs(playerY - this.y)
+
+        let scalingX = 1;
+        let scalingY = 1
+        if (distanceX > distanceY) scalingY = distanceY / distanceX;
+        if (distanceX < distanceY) scalingX = distanceX / distanceY;
